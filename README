  /usr/share/doc/exif-0.6.15/COPYING
  /usr/share/doc/exif-0.6.15/ChangeLog
  /usr/share/doc/exif-0.6.15/INSTALL
  /usr/share/doc/exif-0.6.15/NEWS
  /usr/share/doc/exif-0.6.15/README
  /usr/share/locale/de/LC_MESSAGES/exif.mo
  /usr/share/locale/es/LC_MESSAGES/exif.mo
  /usr/share/locale/fr/LC_MESSAGES/exif.mo
  /usr/share/locale/pl/LC_MESSAGES/exif.mo
